	<include name="MetaSetInfos">
		<param name="width">1040</param>
		<definition>
			<control type="grouplist">
				<height>40</height>
				<width>$PARAM[width]</width>
				<itemgap>5</itemgap>
				<orientation>horizontal</orientation>
				<control type="label">
					<label>$INFO[Container(5601).ListItemAbsolute(0).Year]$VAR[SetLastYear, - ]   </label>
					<include>MetaLabel</include>
					<visible>!String.IsEmpty(Container(5601).NumItems) + !String.IsEqual(Container(5601).NumItems,0) + [!Container(5601).IsUpdating | !String.StartsWith(Container.Folderpath,plugin://)]</visible>
				</control>
				<control type="label">
					<label>$INFO[Container(5601).NumItems,, $LOCALIZE[36901]]   </label>
					<include>MetaLabel</include>
					<visible>!String.IsEmpty(Container(5601).NumItems) + !String.IsEqual(Container(5601).NumItems,0) + [!Container(5601).IsUpdating | !String.StartsWith(Container.Folderpath,plugin://)]</visible>
				</control>
				<control type="label">
					<label>$LOCALIZE[1040]   </label>
					<include>MetaLabel</include>
					<visible>![!String.IsEmpty(Container(5601).NumItems) + !String.IsEqual(Container(5601).NumItems,0) + [!Container(5601).IsUpdating | !String.StartsWith(Container.Folderpath,plugin://)]]</visible>
