<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="image">
		<texture></texture>
		<aspectratio>stretch</aspectratio>
	</default>
	<default type="multiimage">
		<aspectratio align="center">scale</aspectratio>
		<fadetime>400</fadetime>
		<timeperimage>7000</timeperimage>
		<randomize>true</randomize>
	</default>
	<default type="label">
		<label></label>
		<align>left</align>
		<aligny>center</aligny>
		<font>med17</font>
		<textcolor>text_white</textcolor>
		<shadowcolor>text_shadow</shadowcolor>
		<disabledcolor>disabled</disabledcolor>
		<selectedcolor>selected</selectedcolor>
		<scroll>false</scroll>
		<scrollspeed>40</scrollspeed>
	</default>
	<default type="fadelabel">
		<align>left</align>
		<font>med17</font>
		<textcolor>text_white</textcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="button">
		<width min="50">auto</width>
		<height>50</height>
		<font>med22</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<shadowcolor>text_shadow</shadowcolor>
		<textoffsetx>20</textoffsetx>
		<aligny>center</aligny>
		<align>center</align>
		<texturefocus border="10" colordiffuse="button_focus">items/shadow_smallbutton.png</texturefocus>
		<texturenofocus border="10" colordiffuse="grey_button">items/shadow_smallbutton.png</texturenofocus>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="radiobutton">
		<width>200</width>
		<radiowidth>-</radiowidth>
		<radioheight>-</radioheight>
		<textureradioonfocus>-</textureradioonfocus>
		<textureradioonnofocus>-</textureradioonnofocus>
		<textureradioofffocus>-</textureradioofffocus>
		<textureradiooffnofocus>-</textureradiooffnofocus>
		<textureradioondisabled>-</textureradioondisabled>
		<textureradiooffdisabled>-</textureradiooffdisabled>
		<texturefocus border="10" colordiffuse="button_focus">items/shadow_smallbutton.png</texturefocus>
		<texturenofocus border="10" colordiffuse="grey_button">items/shadow_smallbutton.png</texturenofocus>
		<height>50</height>
		<font>med22</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<shadowcolor>text_shadow</shadowcolor>
		<textoffsetx>20</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="textbox">
		<align>left</align>
		<textcolor>text_white</textcolor>
		<shadowcolor>text_shadow</shadowcolor>
		<font>med17</font>
		<autoscroll delay="6000" time="3000" repeat="10000">true</autoscroll>
	</default>
	<default type="progress">
		<texturesliderbackground colordiffuse="background_diffuse">common/white.jpg</texturesliderbackground>
		<texturesliderbar colordiffuse="emby">common/white.jpg</texturesliderbar>
		<texturesliderbarfocus/>
		<textureslidernib/>
		<textureslidernibfocus/>
		<lefttexture></lefttexture>
		<righttexture></righttexture>
		<overlaytexture></overlaytexture>
	</default>
	<default type="scrollbar">
		<texturesliderbackground colordiffuse="bb121314">common/white.jpg</texturesliderbackground>
		<texturesliderbar colordiffuse="lightgrey">common/white.jpg</texturesliderbar>
		<texturesliderbarfocus colordiffuse="lightgrey">common/white.jpg</texturesliderbarfocus>
		<textureslidernib></textureslidernib>
		<textureslidernibfocus></textureslidernibfocus>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
	</default>	
	<default type="togglebutton">
		<font>med20</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<shadowcolor>text_shadow</shadowcolor>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="spincontrol">
		<spinwidth>20</spinwidth>
		<spinheight>12</spinheight>
		<textureup>common/spin_control_up.png</textureup>
		<texturedown>common/spin_control_down.png</texturedown>
		<textureupfocus colordiffuse="emby">common/spin_control_up.png</textureupfocus>
		<texturedownfocus colordiffuse="emby">common/spin_control_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">common/spin_control_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">common/spin_control_down.png</texturedowndisabled>
		<width>77</width>
		<height>50</height>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<subtype>text</subtype>
		<pulseonselect>no</pulseonselect>
		<font>med22</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<shadowcolor>text_shadow</shadowcolor>
	</default>
	<default type="spincontrolex">
		<spinwidth>20</spinwidth>
		<spinheight>12</spinheight>
		<textureup>common/spin_control_up.png</textureup>
		<texturedown>common/spin_control_down.png</texturedown>
		<textureupfocus colordiffuse="emby">common/spin_control_up.png</textureupfocus>
		<texturedownfocus colordiffuse="emby">common/spin_control_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">common/spin_control_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">common/spin_control_down.png</texturedowndisabled>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>no</pulseonselect>
		<font>med22</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<shadowcolor>text_shadow</shadowcolor>
	</default>
	<default type="sliderex">
		<aligny>center</aligny>
		<texturefocus/>
		<texturenofocus/>
		<sliderwidth>150</sliderwidth>
		<sliderheight>10</sliderheight>
		<texturesliderbar colordiffuse="background_diffuse">common/white.jpg</texturesliderbar>
		<textureslidernib>common/white.jpg</textureslidernib>
		<textureslidernibfocus colordiffuse="emby">common/white.jpg</textureslidernibfocus>
		<font>med22</font>
		<textcolor>white</textcolor>
		<focusedcolor>white</focusedcolor>
		<selectedcolor>white</selectedcolor>
		<disabledcolor>disabled</disabledcolor>
		<shadowcolor>text_shadow</shadowcolor>
	</default>
	<default type="slider">
		<texturesliderbar colordiffuse="00ffffff">common/white.jpg</texturesliderbar>
		<textureslidernib colordiffuse="emby">views/player/nib_bubble.png</textureslidernib>
		<textureslidernibfocus colordiffuse="emby">views/player/nib_bubble.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
	</default>
	<default type="edit">
		<font>med17</font>
		<textcolor>text_white</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
		<textoffsetx>7</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="list">
		<scrolltime>300</scrolltime>
	</default>
	<default type="fixedlist">
		<scrolltime>300</scrolltime>
	</default>
	<default type="panel">
		<scrolltime>300</scrolltime>
	</default>
	<default type="wraplist">
		<scrolltime>300</scrolltime>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
		<align>left</align>
	</default>
</includes>
