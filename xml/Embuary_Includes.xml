<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- Expressions -->
	<expression name="ExpInfoDialogActive">[Window.IsVisible(musicinformation) | Window.IsVisible(movieinformation) | Window.IsVisible(addoninformation) | Window.IsVisible(pvrguideinfo) | Window.IsVisible(pvrrecordinginfo) | Window.IsVisible(pictureinfo) | Window.IsVisible(script-script.extendedinfo-DialogVideoInfo.xml) | Window.IsVisible(script-script.extendedinfo-DialogInfo.xml) | Window.IsVisible(script-script.extendedinfo-VideoList.xml)]</expression>
	<!-- FullScreenDimensions -->
	<include name="FullscreenDimensions">
		<width>1920</width>
		<height>1080</height>
	</include>
	<!-- Hidden Control -->
	<include name="HiddenObject">
		<left>-3000</left>
		<top>-3000</top>
		<width>1</width>
		<height>1</height>
		<texturefocus />
		<texturenofocus />
		<label></label>
		<font></font>
	</include>
	<!-- Background -->
	<include name="Background">
		<control type="image">
			<include>FullscreenDimensions</include>
			<texture colordiffuse="background">common/white.jpg</texture>				
		</control>
		<control type="group">
			<control type="videowindow">
				<include>FullscreenDimensions</include>
				<visible>Player.HasVideo + !Skin.HasSetting(HideBackgroundPlayback) + $EXP[HideBackground]</visible>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture>$VAR[BackgroundSource]</texture>
				<fadetime>1000</fadetime>
				<visible>[!Player.HasVideo | Skin.HasSetting(HideBackgroundPlayback)] + !Skin.HasSetting(HideBackground) + !Window.IsVisible(MyPVRguide.xml) + $EXP[HideBackground]</visible>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="background">common/diffuse_white.png</texture>
				<animation effect="fade" start="100" end="100" condition="String.IsEqual(Skin.String(BackgroundAlpha),0) | String.IsEmpty(Skin.String(BackgroundAlpha))" time="0">Conditional</animation>
				<animation effect="fade" start="100" end="97" condition="String.IsEqual(Skin.String(BackgroundAlpha),1)" time="0">Conditional</animation>
				<animation effect="fade" start="100" end="94" condition="String.IsEqual(Skin.String(BackgroundAlpha),2)" time="0">Conditional</animation>
				<animation effect="fade" start="100" end="91" condition="String.IsEqual(Skin.String(BackgroundAlpha),3)" time="0">Conditional</animation>
			</control>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture>holiday/winter/frost.png</texture>
				<aspectratio>stretch</aspectratio>
				<visible>$EXP[HolidayTheme]</visible>
			</control>
		</control>
	</include>
	<!-- No textures -->
	<include name="NoTextures">
		<texturefocus>-</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textureup>-</textureup>
		<texturedown>-</texturedown>
		<textureupfocus>-</textureupfocus>
		<texturedownfocus>-</texturedownfocus>
		<textureupdisabled>-</textureupdisabled>
		<texturedowndisabled>-</texturedowndisabled>
		<textureradioonfocus>-</textureradioonfocus>
		<textureradioonnofocus>-</textureradioonnofocus>
		<textureradioofffocus>-</textureradioofffocus>
		<textureradiooffnofocus>-</textureradiooffnofocus>
		<radiowidth>-</radiowidth>
		<radioheight>-</radioheight>
	</include>
	<!-- Spacer for grouplists -->
	<include name="Spacer">
		<param name="visible" default="false" />
		<param name="width" default="0" />
		<param name="height" default="0" />
		<definition>
			<control type="label">
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<font>invisible</font>
				<label>-</label>
				<visible>$PARAM[visible]</visible>
			</control>
		</definition>
	</include>
