<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol always="true">20</defaultcontrol>
	<menucontrol>6666</menucontrol>
	<controls>
		<include>Background</include>
		<control type="group">
			<include>Window_OpenClose</include>
			<!-- left col -->
			<control type="grouplist">
				<left>135</left>
				<centertop>50%</centertop>
				<width>800</width>
				<height max="940">auto</height>
				<orientation>vertical</orientation>
				<itemgap>20</itemgap>
				<align>center</align>
				<!-- heading -->
				<control type="group">
					<height>50</height>
					<control type="label" id="101">
						<width>800</width>
						<height>48</height>
						<font>Reg24</font>
						<aligny>center</aligny>
						<textoffsetx>5</textoffsetx>
					</control>
					<control type="image">
						<width>100%</width>
						<top>45</top>
						<height>2</height>
						<texture colordiffuse="lightgrey">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<control type="image">
						<width>800</width>
						<top>45</top>
						<height>2</height>
						<texture colordiffuse="emby">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
						<include>WindowOpen_GreenBar</include>
					</control>
				</control>
				<!-- list -->
				<control type="group">
					<height>770</height>
					<control type="image">
						<height>770</height>
						<texture colordiffuse="thumb_background">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<control type="list" id="20">
						<centerleft>50%</centerleft>
						<top>10</top>
						<height>750</height>
						<onup>6666</onup>
						<onleft>900</onleft>
						<onright>21</onright>
						<ondown>noop</ondown>
						<pagecontrol>60</pagecontrol>
						<scrolltime>200</scrolltime>
						<itemlayout height="50" width="800">
							<control type="image">
								<height>50</height>
								<width>50</width>
								<left>5</left>
								<bordersize>5</bordersize>
								<texture background="true">$INFO[ListItem.Icon]</texture>
							</control>
							<control type="label">
								<right>20</right>
								<left>60</left>
								<height>50</height>
								<aligny>center</aligny>
								<font>Reg24</font>
								<label>$INFO[ListItem.Label]$INFO[ListItem.Label2,[COLOR softgrey] [,][/COLOR]]</label>
							</control>
						</itemlayout>
						<focusedlayout height="50" width="800">
							<control type="image">
								<width>100%</width>
								<height>50</height>
								<texture colordiffuse="list_hover">common/white.jpg</texture>
								<visible>Control.HasFocus(20)</visible>
							</control>
							<control type="image">
								<height>50</height>
								<width>50</width>
								<left>5</left>
								<bordersize>5</bordersize>
								<texture background="true">$INFO[ListItem.Icon]</texture>
							</control>
							<control type="label">
								<right>20</right>
								<left>60</left>
								<height>50</height>
								<aligny>center</aligny>
								<font>Reg24</font>
								<label>$INFO[ListItem.Label]$INFO[ListItem.Label2,[COLOR softgrey] [,][/COLOR]]</label>
							</control>
						</focusedlayout>
					</control>
					<control type="scrollbar" id="60">
						<right>0</right>
						<top>0</top>
						<bottom>0</bottom>
						<width>4</width>
						<orientation>vertical</orientation>
						<showonepage>false</showonepage>
						<texturesliderbackground colordiffuse="00ffffff">common/white.jpg</texturesliderbackground>
						<texturesliderbar colordiffuse="softgrey">common/white.jpg</texturesliderbar>
					</control>
				</control>
			</control>
			<!-- right col -->
			<control type="grouplist">
				<right>135</right>
				<centertop>50%</centertop>
				<width>800</width>
				<height max="940">auto</height>
				<orientation>vertical</orientation>
				<itemgap>20</itemgap>
				<align>center</align>
				<!-- heading -->
				<control type="group">
					<height>50</height>
					<control type="label" id="102">
						<width>800</width>
						<height>48</height>
						<font>Reg24</font>
						<aligny>center</aligny>
						<textoffsetx>5</textoffsetx>
					</control>
					<control type="image">
						<width>100%</width>
						<top>45</top>
						<height>2</height>
						<texture colordiffuse="lightgrey">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<control type="image">
						<width>100%</width>
						<top>45</top>
						<height>2</height>
						<texture colordiffuse="emby">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
						<include>WindowOpen_GreenBar</include>
					</control>
				</control>
				<!-- list -->
				<control type="group">
					<height>770</height>
					<control type="image">
						<height>770</height>
						<texture colordiffuse="thumb_background">common/white.jpg</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<control type="list" id="21">
						<centerleft>50%</centerleft>
						<top>10</top>
						<height>750</height>
						<onup>6666</onup>
						<onleft>20</onleft>
						<onright>noop</onright>
						<ondown>noop</ondown>
						<pagecontrol>70</pagecontrol>
						<scrolltime>200</scrolltime>
						<itemlayout height="50" width="800">
							<control type="image">
								<height>50</height>
								<width>50</width>
								<left>5</left>
								<bordersize>5</bordersize>
								<texture background="true">$INFO[ListItem.Icon]</texture>
							</control>
							<control type="label">
								<right>20</right>
								<left>60</left>
								<height>50</height>
								<aligny>center</aligny>
								<font>Reg24</font>
								<label>$INFO[ListItem.Label]$INFO[ListItem.Label2,[COLOR softgrey] [,][/COLOR]]</label>
							</control>
						</itemlayout>
						<focusedlayout height="50" width="800">
							<control type="image">
								<width>100%</width>
								<height>50</height>
								<texture colordiffuse="list_hover">common/white.jpg</texture>
								<visible>Control.HasFocus(21)</visible>
							</control>
							<control type="image">
								<height>50</height>
								<width>50</width>
								<left>5</left>
								<bordersize>5</bordersize>
								<texture background="true">$INFO[ListItem.Icon]</texture>
							</control>
							<control type="label">
								<right>20</right>
								<left>60</left>
								<height>50</height>
								<aligny>center</aligny>
								<font>Reg24</font>
								<label>$INFO[ListItem.Label]$INFO[ListItem.Label2,[COLOR softgrey] [,][/COLOR]]</label>
							</control>
						</focusedlayout>
					</control>
					<control type="scrollbar" id="70">
						<right>0</right>
						<top>0</top>
						<bottom>0</bottom>
						<width>4</width>
						<orientation>vertical</orientation>
						<showonepage>false</showonepage>
						<texturesliderbackground colordiffuse="00ffffff">common/white.jpg</texturesliderbackground>
						<texturesliderbar colordiffuse="softgrey">common/white.jpg</texturesliderbar>
					</control>
				</control>
			</control>
		</control>
		<!-- header bar -->
		<include content="HeaderBar">
			<param name="id">20</param>
			<param name="auto_hide">false</param>
		</include>
		<!-- side panel -->
		<include content="SidePanel">
			<param name="onright">20</param>
		</include>
	</controls>
</window>
