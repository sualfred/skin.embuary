<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- Loading item -->
	<include name="LoadingWidget">
		<param name="focused">false</param>
		<param name="top">94</param>
		<param name="left">70</param>
		<definition>
			<control type="group">
				<visible>Container($PARAM[id]).IsUpdating + !Integer.IsGreater(Container($PARAM[id]).NumItems,0)</visible>
				<left>$PARAM[left]</left>
				<top>$PARAM[top]</top>
				<control type="group">
					<animation type="Conditional" reversible="false" condition="Control.HasFocus($PARAM[id])">
						<effect type="zoom" end="110" time="75" tween="sine" easing="out" center="auto" />
						<effect type="zoom" end="90.9090909" time="225" tween="sine" delay="100" easing="out" center="auto" />
					</animation>
					<width>$PARAM[width]</width>
					<height>$PARAM[height]</height>
					<control type="image">
						<top>-2</top>
						<left>-2</left>
						<width>$PARAM[shadow_width]</width>
						<height>$PARAM[shadow_height]</height>
						<texture border="20,20,20,20">items/shadow.png</texture>
					</control>
					<control type="image">
						<width>$PARAM[width]</width>
						<height>$PARAM[height]</height>
						<texture colordiffuse="thumb_background_fallback_secondary">common/white.jpg</texture>
						<aspectratio scalediffuse="false">stretch</aspectratio>
					</control>
					<include>WidgetLoadingSpinner</include>
					<control type="group">
						<visible>Control.HasFocus($PARAM[id])</visible>
						<animation reversible="false" effect="fade" start="0" end="100" time="500" tween="linear">Visible</animation>
						<animation reversible="false" effect="fade" start="100" end="0" time="0">Hidden</animation>
						<control type="image">
							<texture>items/inner_shadow.png</texture>
							<aspectratio>stretch</aspectratio>
						</control>
						<control type="image">
							<left>-12</left>
							<top>-7</top>
							<width>$PARAM[focus_width]</width>
							<height>$PARAM[focus_height]</height>
							<texture border="20,20,20,20" colordiffuse="$VAR[AccentColor]">items/focus.png</texture>
							<aspectratio>stretch</aspectratio>
							<animation reversible="false" effect="fade" start="0" end="100" time="500" tween="linear" condition="Control.HasFocus($PARAM[id])">Conditional</animation>
							<animation reversible="false" effect="fade" start="100" end="0" time="0" condition="!Control.HasFocus($PARAM[id])">Conditional</animation>
						</control>
					</control>
				</control>
			</control>
		</definition>
	</include>

